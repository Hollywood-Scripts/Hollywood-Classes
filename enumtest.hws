@INCLUDE "enum.hws"

/*
** Unit test
*/
Local test=Enum.new(0,"illegal","a","b","c")
test:dump()
Local test2=test:extend("d","e","f")
test2:dump()
Assert(test._end=3)
Assert(test._start=0)
Assert(test2._end=6)
Assert(test2._start=test._start)
test:append("d","e","f")
test:dump()
Assert(test._end=6)
DebugPrint("Success")
End
